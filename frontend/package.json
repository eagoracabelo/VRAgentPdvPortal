{
  "name": "vragentepdvporta-ui",
  "version": "18.1.42",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --port=4200 --proxy-config proxy.conf.json",
    "start:dev": "ng serve --host=0.0.0.0 --port=4200 --disable-host-check --poll=2000 --proxy-config proxy.conf.json",
    "build:app": "ng build --configuration production --aot --output-hashing=all --source-map=false --named-chunks=false --base-href /",
    "build:VRDesignGuide": "npm run build --prefix projects/design-guide/",
    "build:VRComponents": "ng build @vrsoftbr/vr-components --configuration production",
    "build:VRDatatables": "npm run build:VRComponents && ng build @vrsoftbr/vrc-datatables --configuration production && npm run gulp:VRDatatables",
    "gulp:VRDatatables": "gulp copyDocuments --gulpfile ./projects/datatables/gulpfile.js",
    "build:VRTable": "ng build @vrsoftbr/vr-table --configuration production && npm run gulp:VRTable",
    "gulp:VRTable": "gulp copyDocuments --gulpfile ./projects/table/gulpfile.js",
    "test:VRComponents": "ng test @vrsoftbr/vr-components --no-watch --code-coverage",
    "test:VRComponents:watch": "ng test @vrsoftbr/vr-components --watch=true --code-coverage",
    "test:VRComponents:single": "ng test @vrsoftbr/vr-components --watch=true --code-coverage --include",
    "test:VRDatatables": "ng test @vrsoftbr/vrc-datatables --no-watch --code-coverage",
    "test:VRDatatables:watch": "ng test @vrsoftbr/vrc-datatables --watch=true --code-coverage",
    "test:VRDatatables:single": "ng test @vrsoftbr/vrc-datatables --watch=true --code-coverage --include",
    "test:VRTable": "ng test @vrsoftbr/vr-table --no-watch --code-coverage",
    "test:VRTable:watch": "ng test @vrsoftbr/vr-table --watch=true --code-coverage",
    "test:VRTable:single": "ng test @vrsoftbr/vr-table --watch=true --code-coverage --include",
    "test:lib": "ng test lib --no-watch --code-coverage",
    "test:lib:watch": "ng test lib --watch=true --code-coverage",
    "test:lib:single": "ng test lib --watch=true --code-coverage --include",
    "lint:lib": "eslint --color --max-warnings=0 --no-warn-ignored projects/**/*.ts || exit 1",
    "lint:lib:fix": "eslint --color --fix projects/**/src/lib/**/*.ts",
    "stylelint:lib": "stylelint ./projects/**/*.scss",
    "format:lib": "prettier --check \"projects/**/src/**/*.{ts,html}\" || exit 1",
    "format:lib:fix": "prettier --write \"projects/**/src/**/*.{ts,html}\"",
    "format": "prettier --check \"src/**/*.{ts,html}\" ",
    "format:fix": "prettier --write \"src/**/*.{ts,html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.13",
    "@angular/cdk": "18.2.14",
    "@angular/common": "18.2.13",
    "@angular/compiler": "18.2.13",
    "@angular/core": "18.2.13",
    "@angular/forms": "18.2.13",
    "@angular/platform-browser": "18.2.13",
    "@angular/platform-browser-dynamic": "18.2.13",
    "@angular/router": "18.2.13",
    "@ngneat/cashew": "4.1.0",
    "@vrsoftbr/vr-file-export": "1.1.7",
    "angular-imask": "7.6.1",
    "codemirror": "5.65.18",
    "dexie": "4.0.8",
    "dexie-observable": "4.0.1-beta.13",
    "jspdf": "3.0.0",
    "lodash": "4.17.21",
    "ngx-markdown": "18.1.0",
    "posthog-js": "1.231.0",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.12",
    "@angular-eslint/eslint-plugin-template": "18.4.2",
    "@angular/cli": "18.2.12",
    "@angular/compiler-cli": "18.2.13",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.15.0",
    "@types/codemirror": "5.60.15",
    "@types/dexie": "1.3.1",
    "@types/eslint": "9.6.1",
    "@types/jasmine": "5.1.0",
    "@types/jspdf": "2.0.0",
    "@types/lodash": "4.17.7",
    "@types/node": "20.16.0",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "angular-eslint": "18.4.2",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-sonarjs": "2.0.4",
    "gulp": "5.0.0",
    "gulp-rename": "2.0.0",
    "jasmine-core": "5.1.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-firefox-launcher": "2.1.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-time-stats-reporter": "0.1.0",
    "ng-packagr": "18.2.0",
    "prettier": "3.3.3",
    "stylelint": "16.9.0",
    "stylelint-config-recommended": "14.0.1",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-scss": "6.5.1",
    "typescript": "5.5.4"
  }
}
